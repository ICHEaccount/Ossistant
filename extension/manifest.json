{
    "manifest_version": 3,
    "name": "Ossistant",
    "description": "ossistant test",
    "version": "1.0",
    "icons": {
        "16": "image_test/ossistant.png"
    },
    "action": {
        "default_icon": {              
        "16": "image_test/ossistant.png" 
        },
        "default_title": "Click test",   
        "default_popup": "popup_create.html"
    },
    "background": {
        "service_worker": ["background.js", "popup.js"]
    },
    "permissions": ["activeTab", "contextMenus", "storage", "http://127.0.0.1:*"],
    "host_permissions": ["*://*/*"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }],

    "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self';"

      
}